.title{
	margin: 0;
	line-height: 1.2rem;
	font-size: .36rem;
	text-align: center;
	font-weight: bold;
}
.mui-input-group .mui-input-row{
	height: 1.6rem;
}
.mui-input-row p{
	line-height: .8rem;
}
.mui-input-row p img{
	display: inline-block;
	height: .4rem;
	vertical-align: top;
	margin: .2rem 0;
}
.searchmore .mui-content{
	width: 100%;
	height: 100%;
	background: url(../images/bgmore.png) no-repeat center bottom;
	background-size: 100%;
}

.mui-input-row .mui-input-clear~.mui-icon-clear{
	top: 1rem;
}
.step{
	padding: 12px .45rem 0;
	background-color: #FFFFFF;
	display: none;
}
.step.on{
	display: block;
}
.facetitle p{
	line-height: .8rem;
	text-align: left;
}
.facetitle p img{
	display: inline-block;
	height: .4rem;
	vertical-align: top;
	margin: .2rem 0;
}
.facetitle p span{
	display: inline-block;
	height: .4rem;
	line-height: .4rem;
	vertical-align: middle;
	font-size: .32rem;
	color: #222222;
}

.facetitle .model{
	height: 2.5rem;
	padding: .2rem 0 .3rem;
}
.facetitle .model li{
	width: 3.2rem; 
}
.facetitle .model li img{
	width: 3.2rem;
	height: 2rem;
	display: block;
	border-radius: .1rem;
}
.facetitle .model li:last-child img{
	width: 2rem;
	height: 2rem;
	display: block;
	margin: 0 auto;
	border-radius: 50%;
}

.facetitle .mui-input-group{
	padding: 0;
}
.facetitle .mui-input-group .mui-input-row{
	height: .8rem;
	text-align: left;
}
.facetitle .mui-input-row label{
	width: 1.6rem;
	padding: 0;
	line-height: .8rem;
	height: .8rem;
	font-size: .28rem;
	color: #bcbcbc;
}
.facetitle .mui-input-row label a{
	color: #888;
}
.facetitle .mui-input-row.mui-checkbox{
	text-align: left;
}
.facetitle .mui-input-row.mui-checkbox label{
	width: auto;
	color: #222222;
	padding-left: .4rem;
	line-height: .5rem;
}

.facetitle .mui-input-row input{
	width: 5rem;
	padding: 0;
	line-height: .8rem;
	height: .8rem;
	font-size: .28rem;
	color: #555555;
}
.mui-checkbox input[type=checkbox]{
	width: .36rem;
	height: .36rem;
	line-height: .5rem;
	left: 0;
	top: 0;
}
.mui-checkbox input[type=checkbox]:before{
	font-size: .36rem
}
.facetitle .mui-input-row input.yanzheng{
	padding-right: 2rem;
}
.facetitle .mui-input-group .mui-input-row:after{
	height: 1px;
	left: 0;
	background-color: #f0eff4;
}

.userinfo{
	display: none;
}
.userinfo .userlist{
	width: 100%;  /* 38% */
	float: left !important;
	position: relative;
	/* margin-right: 1%; */
}
.userinfo .userlist:last-child{
	width: 100%;/* 60 */
	float: left !important;
}
.userinfo .userlist input{
	padding: 0;
	line-height: .8rem;
	height: .8rem;
	font-size: .28rem;
	color: #555555;
	border: 0;
	margin: 0;
	border-bottom: 1px solid #f0eff4;
	border-radius: 0;
}
.userinfo .userlist:last-child input{
	width: 36%;
	float: left;
	margin-right: 2%;
}
.userinfo .userlist:last-child a.getcode{
	width: 30%;
	height: .7rem;
	line-height: .7rem;
	color: #FFFFFF;
	background-color: #478ffb;
	font-size: .25rem;
	position: absolute;
	right: 0;
	border-radius: .1rem;
	bottom: 0;
}


.agreements {
	line-height: .6rem;
	height: .6rem;
	text-align: left;
}
.agreements input{
	display: inline-block;
	width: .3rem;
	height: .3rem;
}
.agreements p{
	display: inline-block;
	font-size: .28rem;
	line-height: .6rem;
	vertical-align: top;
}

.facetitle p span i{
	font-style: normal;
	font-size: .26rem;
	margin-left: .1rem;
}
.facetitle .txwarp{
	/* padding: .1rem 0; */
}
.facetitle .txwarp img{
	width: 6.6rem;
	/* height: 4.12rem; */
	height: 3.28rem;
	border-radius: .1rem;
	margin-bottom: .3rem;
}
.facetitle a.getcode{
	width: 2rem;
	height: .64rem;
	bottom: .08rem;
	background: transparent;
}
.facetitle a.getcode img{
	width: 2rem;
	height: .64rem;
}

#tiplist{
	width: 5.2rem;
	max-height: 4rem;
	overflow: scroll;
	position: absolute;
	right: 0;
	top: .8rem;
	background-color: #179EFA;
	z-index: 999;
}
#tiplist ul{
	padding: 0 .2rem;
}
#tiplist ul li{
	line-height: .4rem;
	padding: .1rem 0;
	font-size: .26rem;
	color: #FFFFFF;
	border-bottom: 0.01rem solid #468EFA;
}
#tiplist ul li:last-child{
	border-bottom: 0;
}