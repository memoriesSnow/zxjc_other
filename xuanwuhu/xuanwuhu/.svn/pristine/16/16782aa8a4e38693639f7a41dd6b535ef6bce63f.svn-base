
.mui-table-view{
	padding: 12px 0 0;
}
.mui-scroll-wrapper{
	top: 44px;
}
.mui-table-view-cell{
	padding: .25rem .45rem .25rem .6rem;
	height: 1.5rem;
	margin: 0;
	position: relative;
}
.mui-table-view-cell i{
	width: .1rem;
	height: .1rem;
	border-radius: 50%;
	background-color: #6f6de8;
	position: absolute;
	top: .65rem;
	left: .3rem;
	display: none;
}
.mui-table-view-cell.on i{
	display: block;
}
.mui-table-view-cell::after{
	height: 0;
}
.mui-table-view-cell>a:not(.mui-btn){
	margin: 0;
	padding: 0;
	height: 1rem;
	position: relative;
}
.mui-table-view .mui-media-object.mui-pull-left {
    margin-right: .15rem;
}
.mui-table-view-cell .mui-media-object {
    line-height: .86rem;
    max-width: .86rem;
	width: .86rem;
    height: .86rem;
	border-radius: 50%;
	background: url(../images/msg.png) no-repeat;
	background-size: 100% 100%;
	margin-top: .02rem;
}
.mui-table-view-cell.on .mui-media-object {
    background: url(../images/msgon.png) no-repeat;
	background-size: 100% 100%;
}
.mui-table-view-cell .mui-media-body h4{
	line-height: .5rem;
	font-size: .34rem;
	color: #323232;
	font-weight: 600;
	width: 4rem;
}
.mui-table-view-cell .mui-media-body p{
	font-size: .3rem;
	color: #E0E0E0;
	font-weight: 400;
}
.mui-table-view-cell.on .mui-media-body p{
	color: #666;
}
.mui-table-view p.time{
	line-height: .5rem;
	position: absolute;
	right: 0;
	top: 0;
	font-size: .24rem;
	color: #e0e0e0;
}

.contwrap{
	padding: 0 .4rem;
}
.contwrap .title{
	font-size: .38rem;
	color: #181818;
	line-height: .6rem;
	font-weight: bold;
	padding: .2rem 0;
}
.contwrap .time{
	font-size: .3rem;
	color: #666;
	border-bottom: 0.01rem solid rgba(153, 153, 153, .3);
	padding-bottom: .2rem;
	text-align: left;
}
.contwrap .cont{
	font-size: .32rem;
	color: #333;
	line-height: 2;
	margin: .1rem auto;	
	font-weight: 300;
}