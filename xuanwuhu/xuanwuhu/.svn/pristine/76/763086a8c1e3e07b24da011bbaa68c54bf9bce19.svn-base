.procent{
	font-size: .28rem;
	padding: .1rem .5rem;
	overflow: hidden;
}
.procent .detail{
	width: 6.5rem;
	color: #666666;
	font-size: .28rem;
}
.procent .detail h4{
	margin: 0 0 .05rem;
	font-size: .29rem;
	color: #333;
}
.procent .detail p{
	font-size: .24rem;
	color: #999999; 
	margin: 0;
}
.procent .detail .process{
	height: 1.2rem;
	padding: .3rem 0 0 0;
}
.procent .detail .process .mui-media-object{
	max-width: .7rem;
	width: .7rem;
	height: .7rem;
	position: relative;
	text-align: center;
}
.procent .detail .process .mui-media-object i{
	width: .5rem;
	height: .5rem;
	position: absolute;
	top: 0;
	left: 0;
	display: inline-block;
	border-radius: 50%;
	color: #FFFFFF;
	font-style: normal;
	font-size: .25rem;
	line-height: .5rem;
	z-index: 9;
}
.procent.blue .detail .process .mui-media-object i{
	background: #2d76ff;
}
.procent.green .detail .process .mui-media-object i{
	background: #4bcb9a;
}
.procent.other .detail .process .mui-media-object i{
	background: #6b6aec;
}
.procent.orange .detail .process .mui-media-object i{
	background: #ff7e00;
}
.procent .detail .process .mui-media-object .line{
	width: .01rem;
	height: 1.35rem;
	position: absolute;
	top: -1rem;
	left: .25rem;
	border-left: .02rem solid #eeeeee;
	display: inline-block;
	z-index: 8;
}
.procent .detail .process:first-child .mui-media-object .line{
	display: none;
}
.procent .detail .process .mui-media-body{
	margin-left: .7rem;
}